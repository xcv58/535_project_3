<dataConfig>
        <dataSource type="FileDataSource" encoding="UTF-8" />
        <document>
        <entity name="page"
                processor="XPathEntityProcessor"
                stream="true"
                forEach="/mediawiki/page/"
                url="test.xml"
                transformer="RegexTransformer,DateFormatTransformer"
                >
            <field column="id"        xpath="/mediawiki/page/id" />
            <field column="title"     xpath="/mediawiki/page/title" />
            <field column="revision"  xpath="/mediawiki/page/revision/id" />
            <field column="user"      xpath="/mediawiki/page/revision/contributor/username" />
            <field column="userId"    xpath="/mediawiki/page/revision/contributor/id" />
            <field column="text"      xpath="/mediawiki/page/revision/text" />
	    <field column="get in"      xpath="/mediawiki/page/revision/get_in" />
	    <field column="see"      xpath="/mediawiki/page/revision/see" />
	    <field column="eat"      xpath="/mediawiki/page/revision/eat" />
	    <field column="do"      xpath="/mediawiki/page/revision/do" />
	    <field column="drink"      xpath="/mediawiki/page/revision/drink" />
	    <field column="get around"      xpath="/mediawiki/page/revision/get_around" />
	    <field column="stay safe"      xpath="/mediawiki/page/revision/stay_safe" />
	    <field column="go next"      xpath="/mediawiki/page/revision/go_next" />
	    <field column="sleep"      xpath="/mediawiki/page/revision/sleep" />
	    <field column="buy"      xpath="/mediawiki/page/revision/buy" />
	    <field column="understand"      xpath="/mediawiki/page/revision/understand" />
            <field column="timestamp" xpath="/mediawiki/page/revision/timestamp" dateTimeFormat="yyyy-MM-dd'T'hh:mm:ss'Z'" />
            <field column="$skipDoc"  regex="^#REDIRECT .*" replaceWith="true" sourceColName="text"/>
       </entity>
        </document>
</dataConfig>
